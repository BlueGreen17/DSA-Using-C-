#include<iostream>
using namespace std;

//without extra space
int main()
{
  
  int arr[5] ={1,2,3,4,5};
 
  int l =0;
  int r = 4;//last element

  while(l<r)
  {
    //swap elements
    int temp = arr[l];
    arr[l]= arr[r];
    arr[r]=temp;
    l++;
    r--;


  }

  for(int i=0;i<5;i++)
  {
        cout<<arr[i]<<" ";
  }
}
